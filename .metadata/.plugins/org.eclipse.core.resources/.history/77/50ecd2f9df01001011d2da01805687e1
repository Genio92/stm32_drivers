#include <stdint.h>
#include <STM32F103x8.h>
#include <stm32f103x8_gpio_driver.h>

#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif



GPIO_PinConfig_t red_led={GPIO_PIN_1,GPIO_MODE_OUTPUT_PP,GPIO_SPEED_10M};





int main(void)
{    RCC_GPIOA_CLK_EN();
	MCAL_GPIO_Init(GPIOA, &red_led);
    /* Loop forever */
	while(1){
		MCAL_GPIO_WritePin(GPIOA, red_led->GPIO_PinNumber, )
	}
	for(;;);
}
